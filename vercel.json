{
  "version": 2,
  "builds": [
    {
      "src": "html/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/html/homepage.html"
    },
    {
      "src": "/(.*)",
      "dest": "/html/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    }
  ]
} 